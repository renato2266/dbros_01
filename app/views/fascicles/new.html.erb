<h1>New fascicle</h1>

<% form_for(@fascicle) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :numero %><br />
    <%= f.text_field :numero %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
<p>
	<%= f.label :names %><br />
	<%= f.collection_select :name_ids, @names, :id, :cognome_nome, {}, :multiple => true%>
 </p>
<p>
	<%= f.label :societies %><br />
	<%= f.collection_select :society_ids, @societies, :id, :denominazione, {}, :multiple => true%>
 </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', fascicles_path %>